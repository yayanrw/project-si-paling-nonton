<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SiPalingNonton" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/midnight_blue</item>
        <item name="colorPrimaryVariant">@color/midnight_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/alizarin</item>
        <item name="colorSecondaryVariant">@color/alizarin_dark</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:ignore="ObsoleteSdkInt" tools:targetApi="l">
            ?attr/colorPrimaryVariant
        </item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.SiPalingNonton.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.SiPalingNonton.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>